{
  "version": "0.9.0-*",
  "packOptions": {
    "summary": "OpenTracing integration for ASP.NET Core",
    "description": "This library integrates ASP.NET Core with the OpenTracing API. To fully understand this platform API, it's helpful to be familiar with the OpenTracing project and terminology more generally.\r\n\r\nFor the time being, \"mild\" backwards-incompatible changes may be made without changing the major version number. As OpenTracing and opentracing-csharp mature, backwards compatibility will become more of a priority.",
    "authors": [ "The OpenTracing Authors" ],
    "projectUrl": "https://github.com/opentracing/opentracing-csharp",
    "iconUrl": "https://avatars0.githubusercontent.com/u/15482765",
    "licenseUrl": "https://raw.githubusercontent.com/opentracing/opentracing-csharp/master/LICENSE",
    "tags": [ "opentracing", "distributed-tracing", "tracing", "logging", "aspnetcore", "diagnostics" ],
    "repository": {
      "type": "git",
      "url": "git://github.com/opentracing/opentracing-csharp"
    }
  },

  "dependencies": {
    "OpenTracing": "0.9.0-*",
    "Microsoft.AspNetCore.Http.Abstractions": "1.0.0",
    "Microsoft.AspNetCore.Http.Extensions": "1.0.0"
  },

  "frameworks": {
    "netstandard1.3": {
      "dependencies": {
        "System.Linq": "4.1.0",
        "System.Runtime": "4.1.0",
        "System.Runtime.Extensions": "4.1.0",
        "System.Text.RegularExpressions": "4.1.0"
      }
    }
  },

  "buildOptions": {
    "warningsAsErrors": true
  }
}